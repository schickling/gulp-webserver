{
  "name": "gulp-webserver-io",
  "version": "2.1.0",
  "description": "Gulp plugin to run a local webserver with socket.io debugger",
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/joelchu/gulp-webserver-io.git"
  },
  "keywords": [
    "gulpplugin",
    "webserver",
    "connect",
    "livereload",
    "socket.io",
    "debugger"
  ],
  "author": "Joel Chu <joelckchu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joelchu/gulp-webserver-io/issues"
  },
  "homepage": "https://github.com/joelchu/gulp-webserver-io",
  "devDependencies": {
    "eslint": "^4.2.0",
    "eslint-config-xo-space": "^0.17.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "nsp": "^2.6.3",
    "socket.io-client": "^2.0.3",
    "supertest": "^3.0.0"
  },
  "files": [
    "src",
    "ssl",
    "bin"
  ],
  "dependencies": {
    "chalk": "^2.0.1",
    "connect": "^3.6.2",
    "connect-livereload": "^0.6.0",
    "gulp-util": "^3.0.8",
    "http-proxy-middleware": "^0.17.4",
    "open": "0.0.5",
    "serve-index": "^1.9.0",
    "serve-static": "^1.12.3",
    "socket.io": "^2.0.3",
    "through2": "^2.0.3",
    "tiny-lr": "^1.0.5",
    "watch": "^1.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/fixtures/",
      "/bin/"
    ]
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint . --fix",
    "test": "jest",
    "pt": "DEBUG=1 jest socket.js",
    "testmain": "yarn test --testMatch '**/__tests__/main/*.test.js'"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "jest": true,
      "node": true
    }
  }
}
